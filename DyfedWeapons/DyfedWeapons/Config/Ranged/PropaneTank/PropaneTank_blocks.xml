<configs>
	<append xpath="/blocks">
		
											<!-- Stun Rocket Launcher Ammo -->	

		<block name="propaneTankDyfed">
			<property name="Class" value="CarExplode"/>
			<property class="Explosion">
				<property name="ParticleIndex" value="13"/>
				<property name="RadiusBlocks" value="5"/>
				<property name="BlockDamage" value="10"/>
				<property name="RadiusEntities" value="5"/>
				<property name="EntityDamage" value="250"/>
			</property>
			<property name="ItemTypeIcon" value="explosion"/>
			<property name="Material" value="MLandMine"/>
			<property name="Shape" value="ModelEntity"/>
			<property name="Model" value="#@modfolder:Resources/DyfedWeapons.unity3d?PropaneTankDyfed.prefab"/>
			<property name="ModelOffset" value="0,0,0"/>
			<property name="WaterFlow" value="permitted"/>
			<property name="Place" value="TowardsPlacerInverted"/>
			<property name="Collide" value="sight,movement,melee,bullet,arrow,rocket"/>
			<property name="Path" value="solid"/>
			<property name="IsTerrainDecoration" value="true"/>
			<property name="MaxDamage" value="5"/>
			<drop event="Harvest" name="resourceScrapIron" count="5,10" tag="allToolsHarvest"/>
			<drop event="Destroy" count="0"/>
			<drop event="Fall" name="scrapMetalPile" count="1,5" prob="0.75" stick_chance="1"/>
			<property name="SortOrder1" value="B671"/>
			<property name="SortOrder2" value="0000"/>
			<property name="Tags" value="barrelExplosive"/>
			<property name="FilterTags" value="MC_building,SC_plumbing"/>
		</block>
	</append>
</configs>